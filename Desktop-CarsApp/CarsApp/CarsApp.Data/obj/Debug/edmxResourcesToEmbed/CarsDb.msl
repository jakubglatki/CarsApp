      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Factories">
      <EntityTypeMapping TypeName="IsTypeOf(CarsDb.Data.Factory)">
        <MappingFragment StoreEntitySet="Factories">
