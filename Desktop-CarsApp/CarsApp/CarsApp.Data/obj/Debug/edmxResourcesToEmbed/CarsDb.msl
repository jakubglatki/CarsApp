      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="HandledCarProducts">
      <EntityTypeMapping TypeName="CarsDb.Data.HandledCarProduct">
        <MappingFragment StoreEntitySet="HandledCarProducts">
